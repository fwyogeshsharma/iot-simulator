# IoT Simulator Environment Variables
# Copy this file to .env and update with your own values

# Backend Configuration
JAVA_OPTS=-Xmx512m -Xms256m

# Supabase Configuration
SUPABASE_PROFILES_URL=https://your-project.supabase.co/rest/v1/profiles
SUPABASE_DEVICES_URL=https://your-project.supabase.co/rest/v1/devices
SUPABASE_DEVICE_DATA_URL=https://your-project.supabase.co/rest/v1/device_data
SUPABASE_DEVICE_TYPE_DATA_CONFIGS_URL=https://your-project.supabase.co/rest/v1/device_type_data_configs
SUPABASE_DEVICE_TYPES_URL=https://your-project.supabase.co/rest/v1/device_types
SUPABASE_ELDERLY_PERSONS_URL=https://your-project.supabase.co/rest/v1/elderly_persons
SUPABASE_APIKEY=your-service-role-key-here

# Simulator Configuration
SIMULATOR_DEVICE_INGEST_URL=https://your-project.supabase.co/functions/v1/device-ingest

# Frontend Configuration (if needed)
FRONTEND_PROFILES_APIKEY=your-anon-key-here
